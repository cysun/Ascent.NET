  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "D:/Temp/Ascent/logs/log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": null
