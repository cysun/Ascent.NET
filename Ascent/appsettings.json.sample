{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=ascent;Username=cysun;Password=abcd"
  },
  "OIDC": {
    "Authority": "https://identity.cysun.org",
    "ClientId": "ascent-webapp",
    "ClientSecret": "********"
  },
  "Email": {
    "Host": "localhost",
    "Port": 25,
    "RequireAuthentication": true,
    "Username": "cysun@localhost.localdomain",
    "Password": "abcd"
  },
  "Files": {
    "Directory": "D:/Temp/Ascent/files",
    "AttachmentTypes": [ ".xls", ".xlsx", ".zip" ],
    "TextTypes": [ ".txt", ".java" ]
